<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
<!-- JSF and Spring Integration -->
    <application>
        <el-resolver>
            org.springframework.web.jsf.el.SpringBeanFacesELResolver
        </el-resolver>
    </application>
    <!-- End of JSF and Spring Integration -->
    
    <!-- Navigation Rules -->
    <navigation-rule>
        <navigation-case>
            <from-outcome>studentInfo</from-outcome>
            <to-view-id>/pages/student.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>patientMangement</from-outcome>
            <to-view-id>/pages/patient/PatientManagement.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>patientMangementRedirect</from-outcome>
            <to-view-id>/pages/patient/PatientManagement.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>patientVisitMangement</from-outcome>
            <to-view-id>/pages/patient/PatientVisitManagement.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>patientVisitMangementRedirect</from-outcome>
            <to-view-id>/pages/patient/PatientVisitManagement.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <navigation-case>
            <from-outcome>doctorMangement</from-outcome>
            <to-view-id>/pages/doctor/DoctorManagement.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <navigation-case>
            <from-outcome>doctorMangementRedirect</from-outcome>
            <to-view-id>/pages/doctor/DoctorManagement.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
</faces-config>
